#!/bin/bash
#PBS -l nodes=1:ppn=16:dc2,walltime=0:10:00
#PBS -N app-datanormalize
#PBS -V

cd $PBS_O_WORKDIR

module load matlab

export MATLABPATH=$MATLABPATH:$SCA_SERVICE_DIR
matlab -nodisplay -nosplash -r main

#check for output files
if [ -s dwi.bvecs ] && [ -s dwi.bvals ];
then
	echo 0 > finished
else
	echo "bvecs or bvals  missing"
	echo 1 > finished
	exit 1
fi



